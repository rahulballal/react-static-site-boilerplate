{
  "name": "react-static-site-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:rahulballal/react-static-site-boilerplate.git",
  "author": "Rahul Ballal <rahulballal@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/",
    "serve": "poi src/index.html --serve --open",
    "test": "jest",
    "lint:check": "standard",
    "lint:fix": "prettier-standard 'src/**/*.js'",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "devDependencies": {
    "babel-jest": "^24.7.1",
    "babel-plugin-emotion": "^10.0.9",
    "docz": "^1.0.4",
    "docz-theme-default": "^1.0.4",
    "jest": "^24.7.1",
    "poi": "^12.6.5",
    "prettier": "^1.17.0",
    "prettier-standard": "^9.1.1",
    "react-testing-library": "^6.1.2",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "__DEV__": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "emotion"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  }
}
